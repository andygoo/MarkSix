*{margin:0;padding:0;font-family:'Microsoft yahei';}
html,body{font-size:12px;overflow-x:hidden;}
a{text-decoration:none;}
@media screen and (min-height: 878px){
	body,html{
		height: 100%;
	}
}
.clearFixed:after{
	content:'';
	width:100%;
	height:0;
	clear:both;
	display:block;
	visibility:hidden;
}
#MarkSix{width:100%;background:url(../images/gametable.jpg) no-repeat top;height:100%;overflow:hidden;}
/*Top start*/
/*中间固定，两边平分，使用table特性*/
.MarkSix-top{width:100%;height:132px;position:relative;display:table;}
.MarkSix-top .MarkSix-top-record{background:url(../images/gameRecord.png) no-repeat 100% 0;display:table-cell;margin:auto;position:relative;z-index:10;}
